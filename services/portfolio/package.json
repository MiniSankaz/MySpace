{
  "name": "@stock-portfolio/portfolio",
  "version": "3.0.0",
  "description": "Portfolio Service for Stock Portfolio v3.0 - Stock Trading Features & Market Data",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "market-data:sync": "tsx src/jobs/market-data-sync.ts",
    "portfolio:calculate": "tsx src/jobs/portfolio-calculation.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^12.3.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.13.0",
    "prisma": "^6.13.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/json2csv": "^5.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.2.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "microservices",
    "portfolio",
    "stock-trading",
    "market-data",
    "express",
    "typescript",
    "financial"
  ],
  "author": "Stock Portfolio Team",
  "license": "PRIVATE",
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs",
  "types": "./dist/index.d.ts"
}

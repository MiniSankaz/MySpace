{
  "name": "@port/code-marking-service",
  "version": "1.0.0",
  "description": "Multi-agent code marking and refactoring service with indexing capabilities",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "analyze": "tsx src/cli/analyze.ts",
    "mark": "tsx src/cli/mark.ts",
    "refactor": "tsx src/cli/refactor.ts",
    "index": "tsx src/cli/index.ts",
    "index:build": "tsx src/workers/indexer.worker.ts",
    "index:update": "tsx src/workers/incremental-indexer.worker.ts"
  },
  "keywords": [
    "code-analysis",
    "refactoring",
    "multi-agent",
    "indexing",
    "code-marking"
  ],
  "author": "Port Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "bull": "^4.12.0",
    "redis": "^4.6.11",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "glob": "^10.3.10",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "typescript": "^5.3.3",
    "ts-morph": "^21.0.1",
    "chokidar": "^3.5.3",
    "p-queue": "^7.4.1",
    "minimatch": "^9.0.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12",
    "table": "^6.8.1",
    "ws": "^8.15.1",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/babel__traverse": "^7.20.4",
    "@types/jest": "^29.5.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "prettier": "^3.1.1",
    "prisma": "^5.22.0",
    "nodemon": "^3.0.2"
  }
}
{
  "name": "@stock-portfolio/user-management",
  "version": "3.0.0",
  "description": "User Management Service for Stock Portfolio v3.0 - Authentication & Authorization",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "@prisma/client": "^6.13.0",
    "prisma": "^6.13.0",
    "redis": "^4.6.12",
    "winston": "^3.11.0",
    "dotenv": "^17.2.1",
    "express-rate-limit": "^7.1.5",
    "zod": "^4.0.14",
    "uuid": "^9.0.1",
    "nodemailer": "^6.10.1",
    "jose": "^6.0.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^9.0.7",
    "@types/node": "^24.2.0",
    "typescript": "^5.9.2",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "jest": "^30.0.5",
    "@types/jest": "^29.5.12",
    "eslint": "^9.32.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  },
  "keywords": [
    "microservices",
    "user-management",
    "authentication",
    "authorization",
    "express",
    "typescript",
    "prisma",
    "redis"
  ],
  "author": "Stock Portfolio Team",
  "license": "PRIVATE",
  "engines": {
    "node": ">=18.0.0"
  }
}

    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.1.1",
    "morgan": "^1.10.0",
    "prisma": "^6.13.0",
    "redis": "^4.6.12",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",

{
  "name": "cms-erp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=8192 --expose-gc server.js",
    "dev:v2": "node --max-old-space-size=8192 --expose-gc server-v2.js",
    "dev:next": "next dev -p 4000",
    "build": "next build && tsc src/server/websocket/terminal-ws.ts --outDir dist --esModuleInterop --skipLibCheck",
    "start": "NODE_ENV=production node --max-old-space-size=8192 --expose-gc server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "build:server": "tsc -p tsconfig.server.json",
    "dev:server": "nodemon --exec \"node --max-old-space-size=8192 --expose-gc\" server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run watch:server\" \"npm run dev:server\"",
    "watch:server": "tsc -p tsconfig.server.json --watch",
    "rebuild": "./rebuild.sh",
    "restart": "./quick-restart.sh",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:manual": "echo 'Run manual tests from tests/manual/ directory'",
    "test:all": "npm run test:unit && npm run test:integration",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:mock": "tsx scripts/database/seed-mock-data.ts",
    "db:seed:simple": "tsx scripts/database/seed-simple.ts",
    "cleanup:sessions": "tsx services/ai-assistant/scripts/cleanup-sessions.ts",
    "db:seed:full": "tsx scripts/database/seed-full.ts",
    "generate": "tsx scripts/code-generator.ts",
    "generate:module": "tsx scripts/generators/generate-module-v2.ts",
    "module:new": "tsx scripts/generators/generate-module-v2.ts",
    "module:v3": "tsx scripts/generators/generate-module-v3.ts",
    "generate:module:v3": "tsx scripts/generators/generate-module-v3.ts",
    "gen": "./scripts/generators/generate.sh",
    "gen:enhanced": "tsx scripts/enhanced-generator.ts",
    "sop:check": "node scripts/dev/sop-compliance-validator.js",
    "sop:fix": "node scripts/dev/fix-hardcoded-values.js",
    "sop:fix-preview": "node scripts/dev/fix-hardcoded-values.js --dry-run",
    "smart:generate": "tsx _library/cli/generate.ts",
    "smart:component": "tsx _library/cli/generate.ts component",
    "smart:api": "tsx _library/cli/generate.ts api",
    "smart:model": "tsx _library/cli/generate.ts model",
    "smart:lib": "tsx _library/cli/generate.ts library",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "ai:start": "./scripts/start-with-ai.sh",
    "ai:terminal": "./scripts/claude-terminal.sh",
    "assistant": "npm run ai:start",
    "migrate:terminal": "tsx scripts/migrate-terminal-storage.ts",
    "test:storage": "jest --testPathPattern=storage",
    "perf:test": "tsx scripts/performance-test-terminal-storage.ts",
    "deploy:storage": "./scripts/deploy-terminal-storage.sh",
    "deploy:production": "./scripts/deploy-terminal-storage.sh production HYBRID",
    "deploy:staging": "./scripts/deploy-terminal-storage.sh staging DATABASE",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs terminal-storage",
    "storage:info": "curl http://localhost:4000/api/terminal/storage-info",
    "storage:health": "curl http://localhost:4000/api/terminal/health",
    "memory:check": "node scripts/monitor-memory.js once",
    "memory:monitor": "node scripts/monitor-memory.js monitor 10000",
    "memory:report": "node scripts/monitor-memory.js report",
    "memory:cleanup": "./scripts/cleanup-memory.sh",
    "memory:baseline": "node scripts/monitor-memory.js once > logs/memory-baseline.json",
    "memory:optimize": "./scripts/cleanup-memory.sh --restart",
    "sop:install-hooks": "ln -sf ../../scripts/pre-commit-sop-check.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "pre-commit": "scripts/pre-commit-sop-check.sh"
  },
